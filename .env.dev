# Development Environment Configuration

# Build
BUILD_TARGET=development

# Redis
REDIS_PORT=6379

# API
API_PORT=8000
API_CMD=uvicorn src.api.main:app --host 0.0.0.0 --port 8000 --reload
API_REPLICAS=1

# Workers
LOG_LEVEL=info
WORKER_CONCURRENCY=4
WORKER_REPLICAS=2
LOG_WORKER_CONCURRENCY=2
LOG_WORKER_REPLICAS=1

# Flower
FLOWER_PORT=5555

# Health checks
HEALTHCHECK_INTERVAL=10s

# Restart policy
RESTART_POLICY=no